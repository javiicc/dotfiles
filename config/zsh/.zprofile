# Loaded for login shells (both interactive and non-interactive sessions).
# Subshells that start within the terminal window inherit settings but 
# don't load ~/.zprofile again.


export PATH=$HOME/bin:/usr/local/bin:$PATH
export PATH="$HOME/.yarn/bin:$HOME/.config/yarn/global/node_modules/.bin:$HOME/.local/lib/python3.10/site-packages:usr/bin/python:$PATH"